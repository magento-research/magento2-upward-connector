<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\Framework\App\FrontControllerInterface" type="Magento\UpwardConnector\Controller\Upward" />
    <type name="Magento\Upward\Controller">
        <arguments>
            <argument name="request" xsi:type="object">Magento\Framework\App\RequestInterface</argument>
            <argument name="environment" xsi:type="string">/app/node_modules/@magento/venia-concept/venia-upward.yml</argument>
        </arguments>
    </type>
</config>
